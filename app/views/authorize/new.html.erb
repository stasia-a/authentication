<h1>Please enter following code</h1>


<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

        <%= f.text_field :salt, readonly: true %>

        <%= f.text_field :new_salt %>

        <%= f.submit "Confirm", class: "btn btn-large btn-primary" %>
    <% end %>


    <div><%= @key %></div>

  </div>
</div>



